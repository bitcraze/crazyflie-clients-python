            stop:0.67 blue,
            stop:0.83 magenta,
            stop:1 red);
}

/* --- Disabled groove: greyed out --- */
QSlider::groove:vertical:disabled {
    border-radius: 4px;
    border: 1px solid #444;
    background: qlineargradient(
        x1:0, y1:1, x2:0, y2:0,
        stop:0 #777,
        stop:1 #222
    );
}

/* --- Handle styles --- */
QSlider::handle:vertical:enabled {
    background: #eee;
    border: 1px solid #444;
    width: 10px;
    height: 10px;
    margin: -2px 0;
    border-radius: 5px;
}

